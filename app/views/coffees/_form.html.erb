<%= form_for(@coffee) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :description %>
<%= f.text_area :description %>

<%= f.label :process %>
<%= f.text_field :process %>

<%= f.label :origin %>
<%= f.text_field :origin %>

<%= f.label :grind_id, "Grind" %>
<%= f.collection_select :grind_id, Grind.all, :id, :name, prompt: true %>


<%= f.submit "Submit" %>

<% end %>
